---
import type { CollectionEntry } from "astro:content";
import CourseCard from "./CourseCard.astro";

interface Props {
  course: CollectionEntry<"courses">;
  prices?: {
    old: number;
    new: number;
  };
  holiday?: boolean;
}

const { course, holiday = false, prices } = Astro.props;
---

<dd class="mt-2" id="faq-0">
  <CourseCard {course} {holiday} {prices} />
</dd>
